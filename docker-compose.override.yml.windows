services:
    dns-proxy:
        deploy:
            replicas: 0

    web: !override
        ports:
            - '80:80'
        image: nginx:latest
        volumes:
            - ./:/var/www/html
            - ./docker/web/default.conf:/etc/nginx/conf.d/default.conf
        links:
            - php-fpm
